{
  "name": "TubeInfo",
  "version": "1.0.0",
  "description": "Simple Web Based Youtube Metadata Lookup",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha --recursive --exit",
    "dev": "nodemon -e js,json,css",
    "format": "prettier --write .",
    "check": "eslint . --ext .js",
    "lint": "semistandard --fix \"**/*.js\"",
    "link:fix": "eslint . --ext .js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devXprite/youtube-lookup.git"
  },
  "keywords": [
    "youtube"
  ],
  "author": {
    "name": "DevXprite",
    "email": "devXprite@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devXprite/youtube-lookup/issues"
  },
  "homepage": "https://github.com/devXprite/youtube-lookup#readme",
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "prettier": "2.5.1"
  },
  "nodemonConfig": {
    "ext": "js",
    "delay": "2",
    "ignore": [
      "test/*"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "request": "^2.88.2",
    "serve-static": "^1.15.0"
  }
}
